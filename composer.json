{
  "name": "payex/payex-sdk-php",
  "type": "library",
  "version": "3.0.0",
  "description": "The PayEx SDK for PHP simplifies integrations against PayEx' Ecommerce API platform by providing native PHP interface towards the REST API.",
  "authors": [
    {
      "name": "Sammy Nordström",
      "email": "sammy.nordstrom@visma.com"
     },
     {
       "name": "Richard Birnik",
       "email": "richard@aait.se"
     },
     {
       "name": "Asbjørn Ulsberg",
       "email": "asbjorn.ulsberg@payex.com"
     }
  ],
  "keywords": ["payex", "sdk", "payments", "ecommerce"],
  "homepage": "https://github.com/PayEx/payex-sdk-php",
  "license": "MIT",
  "require": {
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.5 | ~8.0",
    "phpmd/phpmd": "^2.6",
    "squizlabs/php_codesniffer": "^3.4",
    "sebastian/phpcpd": "^4.1",
    "satooshi/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "PayEx\\": "src/PayEx/"
    }
  }
}
